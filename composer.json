{
    "name": "deecodek/rate-limiter",
    "description": "Enterprise-grade rate limiting for Laravel APIs with quotas, analytics, and abuse protection.",
    "type": "library",
    "keywords": ["laravel", "rate-limit", "throttling", "api", "security"],
    "license": "MIT",
    "authors": [
        {
            "name": "Deepak Repositories",
            "email": "deepakddm1@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "predis/predis": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "laravel/pint": "^1.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Deecodek\\RateLimiter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Deecodek\\RateLimiter\\RateLimiterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}